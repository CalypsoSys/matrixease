{
  "executable": "MatrixEase.App",
  "splashscreen": {
    "imageFile": "/wwwroot/matrixease.png"
  },
  "name": "matrixease",
  "description": "See data the way you think about it - An Innovative Visualization and Analysis Tool for large matrix data",
  "author": "Joe Schmitt",
  "singleInstance": false,
  "aspCoreBackendPort": 51266,
  "environment": "Production",
  "build": {
    "appId": "com.matrixease.app",
    "productName": "MatrixEase",
    "copyright": "Copyright Calypso Systems, LLC Â© 2021",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "win": {
      "icon": "../../../Assets/icon.ico"
    },
    "linux": {
      "category": "Utility",
      "icon": "../../../Assets/matrixease.png"
    },
    "directories": {
      "output": "../../../bin/Desktop",
      "buildResources": "Assets"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}
