{
  "executable": "VisAlyzer.App",
  "splashscreen": {
    "imageFile": "/wwwroot/IncTrakIcon.png"
  },
  "name": "VisAlyzer",
  "author": "Joe Schmitt",
  "singleInstance": false,
  "aspCoreBackendPort": 51266,
  "environment": "Production",
  "build": {
    "appId": "com.visalyzer.app",
    "productName": "VisAlyzer",
    "copyright": "Copyright Calypso Systems, LLC Â© 2021",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "win": {
      "icon": "Assets/icon.ico"
    },
    "directories": {
      "output": "../../../bin/Desktop",
      "buildResources": "Assets"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}